<launch>
  <arg name="world" default="src/gz_train1/worlds/testbuild3.sdf" />
  <arg name="file" default="" />
  <arg name="model_string" default="gz_train1/urdf/dif_model.sdf" />
  <arg name="topic" default="" />
  <arg name="entity_name" default="" />
  <arg name="allow_renaming" default="False" />
  <arg name="x" default="0.5" />
  <arg name="y" default="2.0" />
  <arg name="z" default="3.0" />
  <arg name="roll" default="" />
  <arg name="pitch" default="" />
  <arg name="yaw" default="" />
  <gz_spawn_model 
    world="$(var world)"
    file="$(var file)"
    model_string="$(var model_string)"
    topic="$(var topic)"
    entity_name="$(var entity_name)"
    allow_renaming="$(var allow_renaming)"
    x="$(var x)"
    y="$(var y)"
    z="$(var z)"
    roll="$(var roll)"
    pitch="$(var pitch)"
    yaw="$(var yaw)">

  </gz_spawn_model>
   <executable cmd="gz sim -g" />
</launch>